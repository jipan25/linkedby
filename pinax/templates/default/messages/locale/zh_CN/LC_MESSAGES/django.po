#: notification/messages_deleted/notice.html:1
#, python-format
msgid ""
"You have deleted the message <a href=\"%(message_url)s\">%(message)s</a>."
msgstr "已删除消息 <a href=\"%(message_url)s\">%(message)s</a>。"

#: notification/messages_received/notice.html:2
#, python-format
msgid ""
"You have received the message <a href=\"%(message_url)s\">%(message)s</a> "
"from <a href=\"%(sender_url)s\">%(message_sender)s</a>."
msgstr ""
"已接收来自 <a href=\"%(sender_url)s\">%(message_sender)s</a> 的消息 <a href="
"\"%(message_url)s\">%(message)s</a> 。"

#: notification/messages_recovered/notice.html:1
#, python-format
msgid ""
"You have recovered the message <a href=\"%(message_url)s\">%(message)s</a>."
msgstr "已经恢复消息 <a href=\"%(message_url)s\">%(message)s</a>。"

#: notification/messages_replied/notice.html:2
#, python-format
msgid ""
"You have replied to <a href=\"%(message_url)s\">%(message_parent_msg)s</a> "
"from <a href=\"%(recipient_url)s\">%(message_recipient)s</a>."
msgstr ""
"已经回复 <a href=\"%(recipient_url)s\">%(message_recipient)s</a> 的消息 <a "
"href=\"%(message_url)s\">%(message_parent_msg)s</a>。"

#: notification/messages_reply_received/notice.html:2
#, python-format
msgid ""
"<a href=\"%(sender_url)s\">%(message_sender)s</a> has sent you a reply to <a "
"href=\"%(message_url)s\">%(message_parent_msg)s</a>."
msgstr ""
"<a href=\"%(sender_url)s\">%(message_sender)s</a> 回复了 <a href="
"\"%(message_url)s\">%(message_parent_msg)s</a>。"

#: notification/messages_sent/notice.html:2
